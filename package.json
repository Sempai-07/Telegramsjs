{
  "name": "telegramsjs",
  "description": "Telegramsjs is a lightweight Node.js library for creating Telegram bots. It is designed to be a framework and does not rely on third-party libraries such as Telegraf.js or node-telegram-bot-api. The library has a syntax that is similar to Discord.js. This library provides a way to create Telegram bots with ease and flexibility.",
  "version": "1.6.0",
  "main": "./dist/index.js",
  "types": [
    "dist/BaseClient.d.ts",
    "dist/IntentsBitField.d.ts",
    "dist/TelegramBot.d.ts",
    "dist/Util.d.ts",
    "dist/collection/Collection.d.ts",
    "dist/collection/MessageCollector.d.ts",
    "dist/collection/index.d.ts",
    "dist/constanta/ChatActionType.d.ts",
    "dist/constanta/ChatPermission.d.ts",
    "dist/constanta/DocumentTypes.d.ts",
    "dist/constanta/EntityType.d.ts",
    "dist/constanta/Events.d.ts",
    "dist/constanta/GroupPermission.d.ts",
    "dist/constanta/GroupStatus.d.ts",
    "dist/constanta/IntentBits.d.ts",
    "dist/constanta/TelegramIntentBits.d.ts",
    "dist/errorcollection.d.ts",
    "dist/formatters/html.d.ts",
    "dist/formatters/index.d.ts",
    "dist/formatters/markdownv.d.ts",
    "dist/helpers/CombinedClass.d.ts",
    "dist/index.d.ts",
    "dist/Markup.d.ts",
    "dist/request.d.ts"
  ],
  "license": "MIT",
  "files": [
    "./dist",
    "./test",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": [
      {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./dist/index.js"
    ],
    "./markup": [
      {
        "default": "./dist/Markup.js",
        "types": "./dist/Markup.d.ts"
      }
    ],
    "./html": [
      {
        "default": "./dist/formatters/html.js",
        "types": "./dist/formatters/html.d.ts"
      },
      "./dist/formatters/html.js"
    ],
    "./markdownv": [
      {
        "default": "./dist/formatters/markdownv.js",
        "types": "./dist/formatters/markdownv.d.ts"
      },
      "./dist/formatters/markdownv.js"
    ],
    "./package.json": "./package.json"
  },
  "keywords": [
    "javascript",
    "telegram",
    "library",
    "api",
    "bot",
    "ts"
  ],
  "author": "Sempai<zadmanthegreat@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sempai-07/Telegramsjs"
  },
  "bugs": {
    "url": "https://github.com/Sempai-07/telegramsjs/issues"
  },
  "homepage": "https://telegramsjs.surge.sh",
  "dependencies": {
    "axios": "^1.4.0",
    "querystring": "^0.2.1"
  },
  "devDependencies": {
    "@telegram.ts/types": "^1.3.1",
    "@types/node": "^20.4.1"
  }
}
