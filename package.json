{
  "name": "telegramsjs",
  "description": "Telegramsjs is a lightweight Node.js library for creating Telegram bots. It is designed to be a framework and does not rely on third-party libraries such as Telegraf.js or node-telegram-bot-api. The library has a syntax that is similar to Discord.js. This library provides a way to create Telegram bots with ease and flexibility.",
  "version": "1.0.0",
  "main": "./src/index.js",
  "types": [
    "src/constanta/ChatActionType.d.ts",
    "src/constanta/ChatPermission.d.ts",
    "src/constanta/EntityType.d.ts",
    "src/constanta/Events.d.ts",
    "src/constanta/DocumentTypes.d.ts",
    "src/constanta/GroupPermission.d.ts",
    "src/constanta/GroupStatus.d.ts",
    "src/constanta/IntentBits.d.ts",
    "src/constanta/QueryString.d.ts",
    "src/constanta/TelegramIntentBits.d.ts",
    "src/replyMarkup/Markup.d.ts",
    "src/replyMarkup/Keyboard.d.ts",
    "src/MessageCollector.d.ts",
    "src/BaseClient.d.ts",
    "src/errorcollection.d.ts",
    "src/index.d.ts",
    "src/IntentsBitField.d.ts",
    "src/LocalSession.d.ts",
    "src/request.d.ts",
    "src/TelegramBot.d.ts",
    "src/Util.d.ts",
    "src/session/CashSession.d.ts",
    "src/session/SessionTypes.d.ts",
    "src/session/SimpleStorage.d.ts"
  ],
  "license": "MIT",
  "files": [
    "./src",
    "./test",
    "./starting",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": [
      {
        "default": "./src/index.js",
        "types": "./src/index.d.ts"
      },
      "./src/index.js"
    ],
    "./session": [
      {
        "default": "./src/session/index.js",
        "types": "./src/session/index.d.ts"
      },
      "./src/session/index.js"
    ],
    "./markup": [
      {
        "default": "./src/replyMarkup/index.js",
        "types": "./src/replyMarkup/index.d.ts"
      }
    ],
    "./html": [
      {
        "default": "./dist/formatters/html.js",
        "types": "./dist/formatters/html.d.ts"
      },
      "./dist/formatters/html.js"
    ],
    "./markdownv": [
      {
        "default": "./dist/formatters/markdownv.js",
        "types": "./dist/formatters/markdownv.d.ts"
      },
      "./dist/formatters/markdownv.js"
    ],
    "./package.json": "./package.json"
  },
  "keywords": [
    "javascript",
    "telegram",
    "library",
    "api",
    "bot",
    "ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Sempai-07/Telegramsjs"
  },
  "bugs": {
    "url": "https://github.com/Sempai-07/telegramsjs/issues"
  },
  "homepage": "https://telegramsjs.surge.sh",
  "dependencies": {
    "querystring": "^0.2.1",
    "typedoc-plugin-markdown": "^3.15.3"
  },
  "devDependencies": {
    "@types/node": "^20.2.6"
  }
}
